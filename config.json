{
  "database_path": "notes.db",
  "font_family": "JetBrains Mono",
  "font_size": 13,
  "hotkeys": {
    "global": {
      "show_window": "<alt>+s"
    },
    "local": {
      "toggle_focus": "F3",
      "add_note": "F4",
      "move_note": "F6",
      "delete_note": "F8",
      "settings": "Ctrl+,",
      "navigate_up": "Alt+Up",
      "navigate_down": "Alt+Down",
      "navigate_left": "Alt+Left",
      "navigate_right": "Alt+Right",
      "navigate_page_up": "Alt+PgUp",
      "navigate_page_down": "Alt+PgDown"
    }
  },
  "window_geometry": {
    "width": 1253,
    "height": 765,
    "is_maximized": false
  },
  "splitter_sizes": [
    216,
    1032
  ]
}